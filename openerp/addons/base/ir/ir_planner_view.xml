                        <option>Taffy</option>
                        <option selected="selected">Caramel</option>
                        <option>Fudge</option>
                        <option>Cookie</option>
                    </select>
                    <div class="my_slection"></div>

                    <script>
                        $( "select" )
                        .change(function () {
                        var str = "";
                        $( "select option:selected" ).each(function() {
                        str += $( this ).text() + " ";
                        });
                        $( ".my_slection" ).text( str );
                        })
                        .change();
                    </script>
            </html>
