<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="blog_contact_search">
        <ul class="dropdown-menu contact_menu" role="menu">
            <li class="zzz"><input type="email"  class="xxx" placeholder="Search contact"/></li>
            <t t-foreach="contacts" t-as="item">
                <li class="yyy"><t t-esc="item.name"></t></li>
            </t>
        </ul>
    </t>

    <t t-name="blog_contact_search_update">
        <t t-foreach="contacts" t-as="item">
            <li class="yyy"><t t-esc="item.name"></t></li>
        </t>
    </t>
</templates>