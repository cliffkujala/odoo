<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="website_version_rule_snapshot" model="ir.rule">
            <field name="name">Versions</field>
            <field name="model_id" ref="model_website_version_snapshot"/>
            <field name="domain_force">['|',('website_id', '=', 1),('website_id', '=', False)]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_unlink" eval="False"/>
        </record>

        <record id="website_version_rule_website" model="ir.rule">
            <field name="name">Versions</field>
            <field name="model_id" ref="website.model_website"/>
            <field name="domain_force">[('id', '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_unlink" eval="False"/>
        </record>
    </data>
</openerp>